{
  "name": "1tp",
  "version": "0.6.0",
  "description": "One transport protocol to rule them all -- offering a net-like stream API on top of various communication protocols",
  "main": "index.js",
  "keywords": [
    "transport",
    "microminion",
    "nat",
    "udp",
    "tcp",
    "turn"
  ],
  "author": {
    "name": "Nico Janssens",
    "email": "nico.b.janssens@gmail.com"
  },
  "contributors": [
    {
      "name": "Thomas Delaet",
      "email": "thomas@delaet.org"
    }
  ],
  "license": "MIT",
  "engines": {
    "node": ">=4.0.0"
  },
  "browser": {
    "dgram": "chrome-dgram",
    "net": "chrome-net",
    "netstring": "my-temp-netstring",
    "winston": "./lib/logger"
  },
  "dependencies": {
    "chrome-dgram": "^3.0.0",
    "chrome-net": "^3.3.0",
    "debug": "^2.2.0",
    "hat": "0.0.3",
    "merge": "^1.2.0",
    "nat-upnp": "^1.0.0",
    "netstring": "^0.3.0",
    "my-temp-netstring": "https://github.com/nicojanssens/node-netstring.git",
    "netstring-stream": "^0.1.2",
    "public-ip": "^1.1.0",
    "q": "^1.4.1",
    "socket.io-client": "1.4.5",
    "turn-js": "*",
    "winston": "^2.2.0",
    "winston-meta-wrapper": "^1.0.0"
  },
  "devDependencies": {
    "apprtc-socket": "*",
    "babel-preset-es2015": "^6.13.2",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.2.0",
    "envify": "^3.4.1",
    "merge": "^1.2.0",
    "mocha": "^2.4.5",
    "once": "1.x",
    "publish": "^0.6.0",
    "through2": "^0.6.5",
    "uglify-js": "^2.7.0",
    "yargs": "^3.32.0",
    "yn": "^1.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/microminion/1tp.git"
  },
  "scripts": {
    "build": "browserify -s OneTP -e -t [ babelify --global --presets [ es2015 ] ] ./ | uglifyjs -c warnings=false -m > 1tp.min.js",
    "build-debug": "browserify -s OneTP -e ./ > 1tp.debug.js",
    "size": "npm run build && cat 1tp.min.js | gzip | wc -c",
    "test-node": "./node_modules/.bin/mocha test/*.unit.js test/streams/*.unit.js",
    "clean": "rm -f 1tp.*.js && rm -rf node_modules",
    "2npm": "publish"
  }
}
